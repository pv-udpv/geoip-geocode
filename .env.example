# Example .env file for geoip-geocode
# Copy to .env and customize
CLICKHOUSE_HOST = '46.161.5.163'
CLICKHOUSE_PORT = 9000
CLICKHOUSE_USER = 'default'
CLICKHOUSE_PASSWORD = '8YtdBiDD'
CLICKHOUSE_DATABASE = 'default'
# Default provider to use
GEOIP_DEFAULT_PROVIDER=geoip2

# Enable caching
GEOIP_CACHE_ENABLED=false
GEOIP_CACHE_TTL=3600

# MaxMind GeoLite2 / GeoIP2 Database Settings
# Get a free license key at: https://www.maxmind.com/en/geolite2/signup
MAXMIND_ACCOUNT_NAME=your_email@example.com
MAXMIND_LICENSE_KEY=your_maxmind_license_key_here
MAXMIND_ACCOUNT_ID=your_account_id_here
MAXMIND_EDITION_ID=GeoLite2-City  # Options: GeoLite2-City, GeoLite2-Country, GeoLite2-ASN

# IP2Location Database Settings
# Get a free token at: https://lite.ip2location.com/sign-up
IP2LOCATION_TOKEN=your_ip2location_token_here
IP2LOCATION_PRODUCT_CODE=DB11LITE  # Options: DB11LITE, DB11, etc.